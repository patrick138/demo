{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kpndemo"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-patrick.vault.azure.net/"
		},
		"FileServer Patrick_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\52.157.217.166\\share"
		},
		"FileServer Patrick_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "patrick"
		},
		"ssis-trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d25caa3-5b28-47d1-9769-58f894c9d40c/resourceGroups/kpndemo/providers/Microsoft.Storage/storageAccounts/filesharekpn"
		},
		"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "sqlkpn.database.windows.net"
		},
		"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "patrick"
		},
		"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('kpndemo_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"FileServer Patrick_properties_typeProperties_host": {
						"value": "[parameters('FileServer Patrick_properties_typeProperties_host')]"
					},
					"FileServer Patrick_properties_typeProperties_userId": {
						"value": "[parameters('FileServer Patrick_properties_typeProperties_userId')]"
					},
					"ssis-trigger_properties_typeProperties_scope": {
						"value": "[parameters('ssis-trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}